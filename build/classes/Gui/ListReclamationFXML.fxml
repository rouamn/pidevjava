<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gui.ListReclamationFXMLController">
    <stylesheets>
        <URL value="@listreclamationfxml.css" />
    </stylesheets>
   <children>
      <TableView fx:id="table" layoutX="156.0" layoutY="97.0" prefHeight="212.0" prefWidth="441.0">
        <columns>
          <TableColumn fx:id="idRec" prefWidth="28.0" text="id" />
          <TableColumn fx:id="emailRec" prefWidth="106.0" text="email" />
            <TableColumn fx:id="objetRec" prefWidth="91.0" text="Objet" />
            <TableColumn fx:id="contenueRec" prefWidth="215.0" text="contenue De Reclamation" />
        </columns>
      </TableView>
      <VBox prefHeight="400.0" prefWidth="151.0" style="-fx-background-color: #2596be;">
         <children>
            <Label alignment="CENTER" prefHeight="73.0" prefWidth="151.0" text="X-SHAPE" textAlignment="JUSTIFY" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#btnAfficherReclamation" prefHeight="61.0" prefWidth="153.0" style="-fx-background-color: #2596be;" text="liste Reclamation" textFill="WHITE" />
